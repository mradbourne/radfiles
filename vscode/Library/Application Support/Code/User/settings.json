{
  /**
  * Disable telemetry
  */
  "redhat.telemetry.enabled": false,
  "gitlens.telemetry.enabled": false,
  /**
  * Behaviour
  */
  "editor.insertSpaces": true,
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "files.exclude": {
    // "**/node_modules": true,
    // "**/node_modules/**": true,
    // "**/.venv": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/node_modules/**": true,
    "**/.next": true,
    "**/.yarn": true,
    "**/yarn.lock": true,
    "**/.venv": true
  },
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "security.workspace.trust.untrustedFiles": "open",
  "security.workspace.trust.enabled": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": true,
  "search.quickOpen.includeHistory": false,
  "workbench.quickOpen.preserveInput": true,
  "editor.inlineSuggest.enabled": true,
  "editor.defaultFormatter": null,
  /**
  * Fonts
  */
  "editor.fontSize": 14,
  "editor.lineHeight": 18,
  "editor.fontFamily": "JetBrains Mono, Cascadia Mono, Hack, Menlo, Consolas, Monospace",
  "terminal.integrated.fontSize": 14,
  /**
  * GUI
  */
  "workbench.colorTheme": "Default Dark+ Experimental",
  "editor.minimap.enabled": false,
  "editor.codeLens": true,
  "editor.accessibilitySupport": "off",
  "javascript.updateImportsOnFileMove.enabled": "prompt",
  "typescript.updateImportsOnFileMove.enabled": "prompt",
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    80,
    100
  ],
  "explorer.confirmDragAndDrop": false,
  "workbench.editor.untitled.hint": "hidden",
  "editor.wordWrap": "off",
  "editor.renderLineHighlight": "none",
  "workbench.editor.enablePreview": true,
  "workbench.editor.enablePreviewFromQuickOpen": true,
  "workbench.tree.indent": 18,
  "workbench.tree.renderIndentGuides": "always",
  "editor.cursorStyle": "line",
  "security.workspace.trust.banner": "always",
  "explorer.confirmDelete": false,
  "editor.cursorBlinking": "solid",
  "editor.quickSuggestionsDelay": 0,
  "window.openFilesInNewWindow": "on",
  "window.openFoldersInNewWindow": "on",
  "debug.toolBarLocation": "docked",
  "workbench.startupEditor": "none",
  "workbench.list.openMode": "doubleClick",
  "editor.cursorSurroundingLines": 12,
  /**
  * File formatting
  */
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  /**
  * Defaults
  */
  "editor.tabSize": 4,
  "editor.detectIndentation": true,
  "editor.smoothScrolling": true,
  "search.useIgnoreFiles": true,
  "files.autoSave": "onFocusChange",
  "workbench.list.multiSelectModifier": "alt",
  "workbench.settings.editor": "json",
  "editor.suggestSelection": "first",
  /**
  * Git
  */
  "diffEditor.renderSideBySide": false,
  "git.confirmSync": false,
  "git.suggestSmartCommit": false,
  "gitlens.hovers.enabled": false,
  "gitlens.statusBar.enabled": true,
  "gitlens.currentLine.enabled": false,
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.codeLens.enabled": false,
  "gitlens.defaultGravatarsStyle": "identicon",
  "gitlens.views.repositories.showBranchComparison": false,
  "gitlens.views.repositories.showUpstreamStatus": false,
  "gitlens.views.repositories.branches.layout": "list",
  "gitlens.views.commitDetails.files.layout": "tree",
  "gitlens.menus": {
    "editor": {
      "blame": false,
      "clipboard": false,
      "compare": false,
      "history": false,
      "remote": false
    },
    "editorGroup": {
      "blame": false,
      "compare": false
    },
    "editorTab": {
      "clipboard": false,
      "compare": false,
      "history": false,
      "remote": false
    },
    "explorer": {
      "clipboard": false,
      "compare": false,
      "history": false,
      "remote": false
    },
    "scm": {
      "graph": false
    },
    "scmRepositoryInline": {
      "graph": false
    },
    "scmRepository": {
      "authors": false,
      "graph": false
    },
    "scmGroupInline": {
      "stash": false
    },
    "scmGroup": {
      "compare": false,
      "openClose": false,
      "stash": false
    },
    "scmItemInline": {},
    "scmItem": {
      "clipboard": false,
      "compare": false,
      "history": false,
      "remote": false,
      "stash": false
    }
  },
  "gitlens.showWelcomeOnInstall": false,
  "gitlens.views.commits.showBranchComparison": false,
  "gitlens.views.branches.showBranchComparison": false,
  /**
  * Language settings
  */
  // JavaScript / TypeScript
  "js/ts.implicitProjectConfig.checkJs": true,
  "javascript.validate.enable": true,
  "javascript.autoClosingTags": false,
  "typescript.autoClosingTags": false,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  // Python
  "python.analysis.typeCheckingMode": "basic",
  "python.formatting.provider": "black",
  "python.languageServer": "Pylance",
  "python.linting.pylintEnabled": false,
  "python.linting.flake8Enabled": true,
  "python.linting.enabled": true,
  "[python]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "ms-python.python",
    "editor.formatOnType": true
  },
  // Other languages
  "css.validate": false,
  "html.autoClosingTags": false,
  "less.validate": false,
  "scss.validate": false,
  "[dart]": {
    "editor.formatOnType": true,
    "editor.rulers": [
      80
    ],
    "editor.selectionHighlight": false,
    "editor.suggest.snippetsPreventQuickSuggestions": false,
    "editor.suggestSelection": "first",
    "editor.tabCompletion": "onlySnippets",
    "editor.wordBasedSuggestions": false,
  },
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  /**
  * vim
  */
  "vim.leader": "<space>",
  "vim.hlsearch": true,
  "vim.incsearch": true,
  "vim.useSystemClipboard": true,
  "vim.useCtrlKeys": true,
  "vim.highlightedyank.enable": true,
  "vim.showMarksInGutter": true,
  "vim.normalModeKeyBindings": [
    // Fix default functionality
    {
      "before": [
        "ctrl+o"
      ],
      "commands": [
        "workbench.action.navigateBack"
      ]
    },
    {
      "before": [
        "ctrl+i"
      ],
      "commands": [
        "workbench.action.navigateForward"
      ]
    },
    {
      "before": [
        "u"
      ],
      "commands": [
        "undo"
      ]
    },
    {
      "before": [
        "ctrl+r"
      ],
      "commands": [
        "redo"
      ]
    },
    // Window management
    {
      "before": [
        "ctrl+o"
      ],
      "commands": [
        "workbench.action.navigateBack"
      ]
    },
    {
      "before": [
        "ctrl+m",
        "h"
      ],
      "commands": [
        "workbench.action.moveEditorToLeftGroup"
      ]
    },
    {
      "before": [
        "ctrl+m",
        "l"
      ],
      "commands": [
        "workbench.action.moveEditorToRightGroup"
      ]
    },
    {
      "before": [
        "ctrl+m",
        "k"
      ],
      "commands": [
        "workbench.action.moveEditorToAboveGroup"
      ]
    },
    {
      "before": [
        "ctrl+m",
        "j"
      ],
      "commands": [
        "workbench.action.moveEditorToBelowGroup"
      ]
    },
    // Vinegar
    {
      "before": [
        "-"
      ],
      "commands": [
        "workbench.files.action.focusFilesExplorer"
      ]
    },
    // LSP
    {
      "before": [
        "g",
        "D"
      ],
      "commands": [
        "editor.action.revealDeclaration"
      ]
    },
    {
      "before": [
        "g",
        "d"
      ],
      "commands": [
        "editor.action.revealDefinition"
      ]
    },
    {
      "before": [
        "g",
        "t"
      ],
      "commands": [
        "editor.action.goToTypeDefinition"
      ]
    },
    {
      "before": [
        "g",
        "i"
      ],
      "commands": [
        "editor.action.goToImplementation"
      ]
    },
    {
      "before": [
        "g",
        "r"
      ],
      "commands": [
        "editor.action.goToReferences"
      ]
    },
    {
      "before": [
        "K",
      ],
      "commands": [
        "editor.action.showHover"
      ]
    },
    // Telescope
    {
      "before": [
        "<leader>",
        "f"
      ],
      "commands": [
        "workbench.action.quickOpen"
      ]
    },
    {
      "before": [
        "<leader>",
        "b"
      ],
      "commands": [
        "workbench.action.showAllEditors"
      ]
    },
    {
      "before": [
        "<leader>",
        "g"
      ],
      "commands": [
        "workbench.action.findInFiles"
      ]
    },
    {
      "before": [
        "<leader>",
        "s"
      ],
      "commands": [
        "workbench.action.showAllSymbols"
      ]
    },
    // Misc
    {
      "before": [
        "ctrl+k",
      ],
      "commands": [
        "workbench.action.openPreviousRecentlyUsedEditor"
      ]
    },
    {
      "before": [
        "ctrl+j",
      ],
      "commands": [
        "workbench.action.openNextRecentlyUsedEditor"
      ]
    },
  ],
  /**
  * Launch configs
  */
  "launch": {
    "version": "0.2.0",
    "configurations": [
      {
        "type": "chrome",
        "request": "launch",
        "name": "Chrome localhost:3000",
        "url": "http://localhost:3000",
        "webRoot": "${workspaceFolder}",
        "runtimeArgs": [
          "--user-data-dir=/Users/m/source/chrome-user-data",
          "--proxy-server=localhost:8080",
          "--auto-open-devtools-for-tabs"
        ],
      },
    ]
  },
  /**
  * Misc plugins
  */
  "favorites.saveSeparated": true,
  "extensions.experimental.affinity": {
    "vscodevim.vim": 1,
    "vscode.typescript-language-features": 1
  },
}
